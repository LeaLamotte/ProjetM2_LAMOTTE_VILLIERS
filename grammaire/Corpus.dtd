<?xml encoding="UTF-8"?>

<!ELEMENT root (arbre|fichier)+>
<!ATTLIST root
  xmlns CDATA #FIXED ''>

<!ELEMENT arbre (mot)+>
<!ATTLIST arbre
  xmlns CDATA #FIXED ''
  id CDATA #IMPLIED
  n CDATA #IMPLIED>

<!ELEMENT fichier EMPTY>
<!ATTLIST fichier
  xmlns CDATA #FIXED ''
  nom CDATA #REQUIRED>

<!ELEMENT mot (token)+>
<!ATTLIST mot
  xmlns CDATA #FIXED ''
  pos CDATA #REQUIRED>

<!ELEMENT token (#PCDATA)>
<!ATTLIST token
  xmlns CDATA #FIXED ''>
